<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:qccsw="/schema/declaration" targetNamespace="/schema/declaration" elementFormDefault="qualified" attributeFormDefault="qualified">
  <xsd:element name="CustomsDeclaration">
    <xsd:annotation>
      <xsd:documentation>Root Element. Qatar Customs Declaration Message</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="MessageHeader" type="qccsw:DecMsgHeaderType">
          <xsd:annotation>
            <xsd:documentation>Header for the message. Includes information on the sender, date and time</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="DeclarationRecord" maxOccurs="99">
          <xsd:annotation>
            <xsd:documentation>Wrapper of the declaration record. Allowed to submit multiple declarations in one XML file</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Payload">
                <xsd:annotation>
                  <xsd:documentation>Wrapper of declaration payload</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Action" type="qccsw:SystemAction" default="Submit">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of System Process element</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DeclarationHeader" type="qccsw:DeclarationHeader">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of declaration header element</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Party">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper for party list</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Importer" type="qccsw:PartyImporter" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Importer details</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="Exporter" type="qccsw:PartyExporter" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Exporter details</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="ConsignorConsignee" type="qccsw:PartyConsignorConsignee" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Consignor Consignee details</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="BillOfLading" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>Bill of Lading (Transport document) details</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="CargoManifestNumber" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Cargo manifest reference number</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:minLength value="0" />
                                <xsd:maxLength value="35" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="MasterBLNumber" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Master Bill Of Lading Number</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:minLength value="0" />
                                <xsd:maxLength value="35" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="HouseBLNumber" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>House Bill Of Lading Number</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:minLength value="0" />
                                <xsd:maxLength value="35" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <!-- Added "CountryOfLoading" by Debasish -->
                          <xsd:element name="CountryOfLoading" type="qccsw:CountryCodeDataType" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Country of loading Type</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <!-- end by Debasish -->
                          <xsd:element name="PortOfLoading" type="qccsw:PortCode" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Port of loading</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="PortOfLoadingType" type="qccsw:PortType" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Port of loading Type</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="DateofArrivalOrDeparture" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Actual Date and time of arrival of goods (for import) or Expected Date and time of departure (for export)</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:dateTime" />
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="FinalDestination" type="qccsw:CountryCodedType" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Country code of the final destination</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="TransportMeans" type="qccsw:CarrierDetails" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Means of transport details</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="BLNumberOfPackages" type="qccsw:NumberOfPackagesType" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Bill Of Lading Number of Packages</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="BLGrossWeight" type="qccsw:WeightTypeNotFixed" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Gross Weight of Bill Of Lading</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="GatePassNumber" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                              <xsd:documentation>Only for  Land Type Declaration the element is mandatory. Gate Pass Number will be provided by customs whenever vehicle entered in to Qatar</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:minLength value="0" />
                                <xsd:maxLength value="25" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <!-- Added "CountryOfLoading" by Debasish -->
                          <!-- end by Debasish -->
                          <!-- Added by Debasish for Carrier Information for Land port Declaration in BL tab-->
                          <!--<xsd:element name="Carrier Information" type="qccsw:CarrierInformation" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Carrier Information</xsd:documentation>
														</xsd:annotation>
													</xsd:element> -->
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Containers" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of container list</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Container" type="qccsw:ContainerDetails" maxOccurs="999">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of container record</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Seals" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of seals list</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Seal" type="qccsw:SealsDetails" maxOccurs="999">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of seals record</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="CertificateOfOrigins" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of certificate of origin list</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="certificateoforigin" type="qccsw:CertificateOfOriginDetails" minOccurs="0" maxOccurs="999">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of certificate of origin record</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Invoices" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of invoice list</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="NumberOfCOONotAttested" default="0" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Number of Certificate of Origins Not Attested.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:int">
                                <xsd:minInclusive value="0" />
                                <xsd:maxInclusive value="100" />
                                <xsd:fractionDigits value="0" />
                                <xsd:totalDigits value="3" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="NumberOfCOONotOriginal" default="0" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Number of Certificate of Origins Not Original.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:int">
                                <xsd:minInclusive value="0" />
                                <xsd:maxInclusive value="100" />
                                <xsd:fractionDigits value="0" />
                                <xsd:totalDigits value="3" />
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="Invoice" type="qccsw:InvoiceType" minOccurs="0" maxOccurs="999">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of invoice record</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Items">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of items list</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="LineItem" type="qccsw:CustomsGoodsItem" maxOccurs="999">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of item record</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
					<!-- added by reddy for Excise tax CR tab-->
					<xsd:element name="ExciseTaxItems" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of Excise Tax Items list</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ProductDetails" type="qccsw:ProductItems" maxOccurs="999">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of product details record</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
					
					
                    <xsd:element name="Charges" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>Wrapper of Declaration related charges details</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Charge" type="qccsw:ChargeType" minOccurs="0" maxOccurs="99">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of charge record</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Dealing" type="qccsw:StatisticalDeclarationType" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>GCC Statistical declaration details</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DocList" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>List of document attachment</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence maxOccurs="10">
                          <xsd:element name="DocAttachment" type="qccsw:DocAttachmentDataType">
                            <xsd:annotation>
                              <xsd:documentation>Wrapper of the attached document</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="recordID" type="xsd:string" />
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="DecMsgHeaderType">
    <xsd:annotation>
      <xsd:documentation>Definition of message header information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="docType" type="xsd:string" fixed="CUSDEC" />
      <xsd:element name="batchID" type="xsd:string" />
      <xsd:element name="MessageSender" type="qccsw:DecMessageSenderDataType">
        <xsd:annotation>
          <xsd:documentation>Details of the message sender</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MessageDate" type="qccsw:MessageDateDataType" minOccurs="0" />
      <xsd:element name="MessageTime" type="qccsw:MessageTimeDataType" minOccurs="0" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DecMessageSenderDataType">
    <xsd:annotation>
      <xsd:documentation>Definition of Message sender datatype</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="UserId" type="qccsw:UserIDDataType">
        <xsd:annotation>
          <xsd:documentation>user id of the sender</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="PartyCode">
    <xsd:annotation>
      <xsd:documentation>Party Code</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0" />
      <xsd:maxLength value="25" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="UserIDDataType">
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="11" />
      <xsd:maxLength value="22" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MessageDateDataType">
    <xsd:annotation>
      <xsd:documentation>Format: ddmmyyyy</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="8" />
      <xsd:maxLength value="8" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MessageTimeDataType">
    <xsd:annotation>
      <xsd:documentation>Format: hhmm</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="4" />
      <xsd:maxLength value="4" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SystemAction">
    <xsd:annotation>
      <xsd:documentation>System to submit the declaration or keep it as draft</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Draft" />
      <xsd:enumeration value="Submit" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DeclarationHeader">
    <xsd:annotation>
      <xsd:documentation>Header elements for the declaration</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DeclarationType" type="qccsw:DeclarationType">
        <xsd:annotation>
          <xsd:documentation>Type of declarations in Qatar. Allowed 6 types,
						refer to enumeration values</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TempImportApplicationNo" type="qccsw:TempImpAppNo" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Valid and approved temporary import application
						number. Applicable only for temporary import
						declarations</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CustomsOfficeofEntry" type="qccsw:PortCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Port of entry in qatar</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CustomsOfficeofExit" type="qccsw:PortCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Port of exit, for transit declarations</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StatisticalDecFlag" type="qccsw:YesNo" default="N" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Flag to indicate if the declaration is GCC
						statistical declaration</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OnsiteInspectionFlag" type="qccsw:YesNo" default="N" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Flag to indicate if there is a approved onsite
						inspection request</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ContainerTransportIndicator" type="qccsw:YesNo" default="N">
        <xsd:annotation>
          <xsd:documentation>To indicate if its containerized cargo</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StrategicGoods" type="qccsw:YesNo" default="N" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>To indicate if the declaration contains
						stratergic goods</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Remarks" type="qccsw:Remarks" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Additional remarks, if any</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CourierDeclaration" type="qccsw:YesNo" default="N" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>To indicate if the declaration is for
						 Courier Company or not</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExpressCompanies" type="qccsw:ExpressCompaniesType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>To indicate if the company is for 
						Express company or not</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ModeofPayment" type="qccsw:ModeofPaymentType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>To indicate the mode of payment for  
						Courier Declaration</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GuaranteeAccount" type="qccsw:GuaranteeAccountType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>To indicate the declaration is using the Guarantee 
						account as for own or Courier company.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="GuaranteeAccountType">
    <xsd:annotation>
      <xsd:documentation>Types of Guarantee Account,weather it is 
     			 'Express Company(C)' or 'Own Company(O)'</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="C" />
      <xsd:enumeration value="O" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ExpressCompaniesType">
    <xsd:annotation>
      <xsd:documentation>Types of Express Companies</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="4" />
      <xsd:maxLength value="50" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ModeofPaymentType">
    <xsd:annotation>
      <xsd:documentation>Mode of Payments,if it is 'Global Guarantee(G)' 
      						or 'Normal(N)'</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="G" />
      <xsd:enumeration value="N" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="DeclarationType">
    <xsd:annotation>
      <xsd:documentation>Types of declaration</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IMP" />
      <xsd:enumeration value="EXP" />
      <xsd:enumeration value="TRN" />
      <xsd:enumeration value="REE" />
      <xsd:enumeration value="TIM" />
      <xsd:enumeration value="TEX" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TempImpAppNo">
    <xsd:annotation>
      <xsd:documentation>Temporary Import Application Number</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0" />
      <xsd:maxLength value="50" />
    </xsd:restriction>
  </xsd:simpleType>
  <!-- Added "CountryCodeDataType" by Debasish -->
  <xsd:simpleType name="CountryCodeDataType">
    <xsd:annotation>
      <xsd:documentation>Country (coded) element definition. ISO Country code</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="2" />
    </xsd:restriction>
  </xsd:simpleType>
  <!-- end by Debasish -->
  <xsd:simpleType name="PortCode">
    <xsd:annotation>
      <xsd:documentation>Port Code</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="2" />
      <xsd:maxLength value="5" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PortType">
    <xsd:annotation>
      <xsd:documentation>Types of port</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="S" />
      <xsd:enumeration value="L" />
      <xsd:enumeration value="A" />
      <xsd:enumeration value="F" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="YesNo">
    <xsd:annotation>
      <xsd:documentation>Values: Y and N</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="1" />
      <xsd:enumeration value="Y" />
      <xsd:enumeration value="N" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Remarks">
    <xsd:annotation>
      <xsd:documentation>Remarks</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="300" />
      <xsd:minLength value="0" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PartyImporter">
    <xsd:annotation>
      <xsd:documentation>Importer</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ImporterCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Importer Code</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="0" />
            <xsd:maxLength value="25" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ImporterName" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Importer Name</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="0" />
            <xsd:maxLength value="300" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ImporterAddress" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Importer Address</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="0" />
            <xsd:maxLength value="300" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ImporterPhone" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Importer Phone</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="8" />
            <xsd:maxLength value="15" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ImporterIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Importer ID Type</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="PASPRT" />
